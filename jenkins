pipeline {
    agent any 

    stages {
        stage('Build') { 
            steps { 
                sh 'pwd'
		echo "test.....1"
            }
        }
        stage('Test'){
            steps {
                sh 'java -version'
                
            }
        }
        stage('Deploy') {
            steps {
                sh 'ls'
                sh 'pwd'
            }
        }
    }
}
